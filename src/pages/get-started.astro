---
import Layout from '~/layouts/PageLayout.astro';
import GetStartedComponent from '~/components/common/GetStarted';

const metadata = {
  title: 'Get Started',
};
---

<Layout metadata={metadata}>
  <section class="px-4 py-16 sm:px-6 mx-auto lg:px-8 lg:py-20 max-w-4xl">
    <h1 class="font-bold font-heading text-4xl md:text-5xl leading-tighter tracking-tighter mb-8 text-center">
      Get Started
    </h1>
    <GetStartedComponent client:load />
  </section>
</Layout>
